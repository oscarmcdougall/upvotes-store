{
    "env": {
        "es2021": true,
        "browser": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "code": 200,
        "prettier/prettier": "error"
    }
}
